<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
    "http://getahead.org/dwr/dwr30.dtd">

<dwr>
  <allow>
    <create creator="new" javascript="JDate">
        <param name="class" value="java.util.Date"/>
    </create>
    <create creator="new" javascript="FacadeAjax">
        <param name="class" value="facade.FacadeAjax"/>
    </create>
    
    <convert converter="bean" match="model.bean.TipoForca"/>
    <convert converter="bean" match="model.dao.TipoForcaDAO"/>  
      
    <convert converter="bean" match="model.bean.Forca"/>
    <convert converter="bean" match="model.dao.ForcaDAO"/>    
    
    <convert converter="bean" match="model.bean.Om"/>
    <convert converter="bean" match="model.dao.OmDAO"/>    
    
    <convert converter="bean" match="model.bean.PostoGraduacao"/>
    <convert converter="bean" match="model.dao.PostoGraduacaoDAO"/> 
    
    <convert converter="bean" match="model.bean.Usuario"/>
    <convert converter="bean" match="model.dao.UsuarioDAO"/>
    
    <convert converter="bean" match="model.bean.DivisaoSecao"/>
    <convert converter="bean" match="model.dao.DivisaoSecaoDAO"/>
    
    <convert converter="bean" match="model.bean.Setor"/>
    <convert converter="bean" match="model.dao.SetorDAO"/>
    
    <convert converter="bean" match="model.bean.Visitante"/>
    <convert converter="bean" match="model.dao.VisitanteDAO"/>
    
    <convert converter="bean" match="model.bean.Visita"/>
    <convert converter="bean" match="model.dao.VisitaDAO"/>
  </allow>
</dwr>
