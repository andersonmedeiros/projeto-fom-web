        <servlet-name>ExcluirDivisaoSecao</servlet-name>
        <url-pattern>/restrito/divisaosecao/controller.divisaosecao/ExcluirDivisaoSecao</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CadastrarSetor</servlet-name>
        <servlet-class>controller.setor.CadastrarSetor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastrarSetor</servlet-name>
        <url-pattern>/restrito/setor/controller.setor/CadastrarSetor</url-pattern>
    </servlet-mapping>
