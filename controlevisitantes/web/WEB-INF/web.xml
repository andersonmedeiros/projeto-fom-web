        <url-pattern>/restrito/visitante/controller.visitante/CadastrarVisitante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AtualizarVisitante</servlet-name>
        <url-pattern>/restrito/visitante/controller.visitante/AtualizarVisitante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExcluirVisitante</servlet-name>
        <url-pattern>/restrito/visitante/controller.visitante/ExcluirVisitante</url-pattern>
    </servlet-mapping>
