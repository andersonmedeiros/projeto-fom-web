<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <display-name>DWR Servlet</display-name>
        <servlet-name>dwr-invoker</servlet-name>
        <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
        <init-param>
            <param-name>debug</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>CadastrarVisitante</servlet-name>
        <servlet-class>controller.visitante.CadastrarVisitante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AtualizarVisitante</servlet-name>
        <servlet-class>controller.visitante.AtualizarVisitante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExcluirVisitante</servlet-name>
        <servlet-class>controller.visitante.ExcluirVisitante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarOm</servlet-name>
        <servlet-class>controller.organizacaomilitar.CadastrarOm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AtualizarOm</servlet-name>
        <servlet-class>controller.organizacaomilitar.AtualizarOm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExcluirOm</servlet-name>
        <servlet-class>controller.organizacaomilitar.ExcluirOm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarEntrada</servlet-name>
        <servlet-class>controller.visita.CadastrarEntrada</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarSaida</servlet-name>
        <servlet-class>controller.visita.CadastrarSaida</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GerarRelatorioPdf</servlet-name>
        <servlet-class>controller.relatorio.GerarRelatorioPdf</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dwr-invoker</servlet-name>
        <url-pattern>/dwr/*</url-pattern>
    </servlet-mapping>
    <filter>
        <filter-name>FiltroPaginas</filter-name>
        <filter-class>controller.FiltroPaginas</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FiltroPaginas</filter-name>
        <url-pattern>/restrito/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>Autenticador</servlet-name>
        <servlet-class>controller.Autenticador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Autenticador</servlet-name>
        <url-pattern>/Autenticador</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sair</servlet-name>
        <servlet-class>controller.Sair</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sair</servlet-name>
        <url-pattern>/restrito/Sair</url-pattern>
        <url-pattern>/restrito/divisaosecao/Sair</url-pattern>
        <url-pattern>/restrito/organizacaomilitar/Sair</url-pattern>
        <url-pattern>/restrito/setor/Sair</url-pattern>
        <url-pattern>/restrito/usuario/Sair</url-pattern>
        <url-pattern>/restrito/visita/Sair</url-pattern>
        <url-pattern>/restrito/visitante/Sair</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CadastrarUsuario</servlet-name>
        <servlet-class>controller.usuario.CadastrarUsuario</servlet-class>
