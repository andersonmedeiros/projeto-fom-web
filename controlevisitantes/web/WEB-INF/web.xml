        <servlet-name>CadastrarOm</servlet-name>
        <url-pattern>/restrito/organizacaomilitar/controller.organizacaomilitar/CadastrarOm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AtualizarOm</servlet-name>
        <url-pattern>/restrito/organizacaomilitar/controller.organizacaomilitar/AtualizarOm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExcluirOm</servlet-name>
        <url-pattern>/restrito/organizacaomilitar/controller.organizacaomilitar/ExcluirOm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarEntrada</servlet-name>
        <url-pattern>/restrito/visita/controller.visita/CadastrarEntrada</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarSaida</servlet-name>
        <url-pattern>/restrito/visita/controller.visita/CadastrarSaida</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GerarRelatorioPdf</servlet-name>
        <url-pattern>/restrito/visita/controller.relatorio/GerarRelatorioPdf</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
</web-app>
