        <servlet-name>CadastrarDivisaoSecao</servlet-name>
        <url-pattern>/restrito/divisaosecao/controller.divisaosecao/CadastrarDivisaoSecao</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>AtualizarDivisaoSecao</servlet-name>
        <servlet-class>controller.divisaosecao.AtualizarDivisaoSecao</servlet-class>
